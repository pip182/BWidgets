
{
  "user_config": {
    "user": "Alice",
    "theme": "dark"
  },
  "Widget Container1": {
    "title": "System Monitor",
    "geometry": [50, 50, 800, 600],
    "layout": "QGridLayout",
    "widgets": [
      {
        "type": "label",
        "text": "System Monitor",
        "row": 0,
        "column": 0,
        "size": {"width": 400, "height": 50},
        "style": {
          "color": "white",
          "font-size": "18px",
          "font-weight": "bold"
        }
      },
      {
        "type": "latency_chart",
        "target_host": "google.com",
        "interval": 1000,
        "max_points": 180,
        "irregular_factor": 3,
        "row": 1,
        "column": 0,
        "size": {"width": 600, "height": 300},
        "style": {
          "background-color": "#ffffff",
          "font-size": "14px"
        }
      },
      {
        "type": "button",
        "text": "Run Diagnostics",
        "row": 2,
        "column": 0,
        "action": [
          {
            "type": "notification",
            "message": "Diagnostics started."
          }
        ],
        "size": {"width": 200, "height": 50},
        "style": {
          "background-color": "lightblue",
          "font-size": "14px"
        }
      }
    ]
  },
  "Widget Container2": {
    "title": "Diagnostics",
    "geometry": [900, 50, 600, 400],
    "layout": "QVBoxLayout",
    "widgets": [
      {
        "type": "table",
        "data": [
          ["Metric", "Value"],
          ["CPU Usage", "Loading..."],
          ["Memory Usage", "Loading..."]
        ],
        "size": {"width": 400, "height": 200},
        "style": {
          "background-color": "white",
          "font-size": "12px",
          "border": "1px solid #ccc"
        }
      }
    ]
  }
}
